abstract class Geometry
{
		
	package net.blimster.gwt.threejs.core;

	constructors:
		default();
	
	properties:
		Vector3[] vertices;
		Material[] materials;
		Face[] faces;
		UV[3] faceVertexUvs;
		Color[] colors;
		BoundingSphere boundingSphere;
		
	methods:
		computeCentroids();
		computeFaceNormals();
		computeVertexNormals();
	
}